{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seli-app/nextjs-boilerplate.git"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/lib/assets/*",
      "!<rootDir>/lib/store/apis/*",
      "!<rootDir>/lib/store/actions/*",
      "!<rootDir>/lib/i18n/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ],
    "setupFiles": [
      "./enzyme.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seli-app/nextjs-boilerplate/issues"
  },
  "homepage": "https://github.com/seli-app/nextjs-boilerplate#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "dotenv-load": "^1.1.0",
    "i18next": "^15.0.7",
    "i18next-browser-languagedetector": "^3.0.1",
    "next": "^8.0.4",
    "next-env": "^1.1.0",
    "next-redux-saga": "^4.0.1",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.6.1",
    "react-redux": "^6.0.1",
    "react-table": "^6.9.2",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "sweetalert": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "jest-transform-stub": "^2.0.0",
    "redux-mock-store": "^1.5.3"
  }
}
